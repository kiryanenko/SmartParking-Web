<%= bootstrap_form_with(model: [local_assigns[:parking], parking_place], local: true) do |form| %>
  <%= render 'components/form_errors', resource: parking_place %>

  <%= form.number_field :place_id %>
  <%= form.text_field :sensor_id %>
  <%= form.text_field :title %>
  <%= form.text_field :coord %>
  <%= form.check_box :for_disabled %>
  <%= form.check_box :can_book %>

  <%= form.submit %>
<% end %>
