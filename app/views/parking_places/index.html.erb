<p id="notice"><%= notice %></p>

<h1>Parking Places</h1>

<table>
  <thead>
    <tr>
      <th>Place</th>
      <th>Sensor</th>
      <th>Parking</th>
      <th>Title</th>
      <th>Coords</th>
      <th>For disabled</th>
      <th>Booked</th>
      <th>Free</th>
      <th>Connected</th>
      <th>Can book</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @parking_places.each do |parking_place| %>
      <tr>
        <td><%= parking_place.place_id %></td>
        <td><%= parking_place.sensor %></td>
        <td><%= parking_place.parking %></td>
        <td><%= parking_place.title %></td>
        <td><%= parking_place.coords %></td>
        <td><%= parking_place.for_disabled %></td>
        <td><%= parking_place.booked %></td>
        <td><%= parking_place.free %></td>
        <td><%= parking_place.connected %></td>
        <td><%= parking_place.can_book %></td>
        <td><%= link_to 'Show', parking_place %></td>
        <td><%= link_to 'Edit', edit_parking_place_path(parking_place) %></td>
        <td><%= link_to 'Destroy', parking_place, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Parking Place', new_parking_place_path %>
